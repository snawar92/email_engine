<!DOCTYPE html>
<html>
<head>
  <title>Email Engine</title>
  <%= stylesheet_link_tag 'email_engine/email_engine.css', media: 'all' %>
  <%= javascript_include_tag 'email_engine/email_engine.js' %>
  <%= csrf_meta_tags %>
</head>
<body>
  <nav class="navbar">
    <div class="container-fluid">
      <div class="navbar-header">
        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
          <span class="sr-only">Toggle navigation</span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </button>
        <%= link_to emails_path, class: 'navbar-brand' do %>
          <img width="25" height="21" title="" alt="" src="data:image/gif;base64,"/>EmailEngine
        <% end %>
      </div>
      <div id="navbar" class="navbar-collapse collapse">
        <ul class="nav navbar-nav navbar-right date-options">
          <li>
            <span style="display:inline-block; padding: 5px;"><%= check_box_tag :refresh, nil, false, style: "display:inline-block" %><%= label_tag :refresh, 'Refresh', style: "padding: 0px 5px; display:inline-block" %></span>
            <%= text_field_tag :refresh_interval, nil, value: (params[:refesh]||'3000'), class: 'form-control', style: "display:none;width:60px;margin: 0px 20px;" %>
          </li>
          <%= content_tag(:li, link_to("Last Hour", emails_path({last: 'hour', interval: 'minute'})), class: ((params[:last].nil? || params[:last] == 'hour') ? 'active' : '')) %>
          <%= content_tag(:li, link_to("Last Day", emails_path({last: 'day', interval: 'hour'})), class: (params[:last] == 'day' ? 'active' : '')) %>
          <%= content_tag(:li, link_to("Last Month", emails_path({last: 'month', interval: 'day'})), class: (params[:last] == 'month' ? 'active' : '')) %>
          <%= content_tag(:li, link_to("Last Year", emails_path({last: 'year', interval: 'month'})), class: (params[:last] == 'year' ? 'active' : '')) %>
        </ul>
      </div>
    </div>
  </nav>

  <div class="container-fluid">
    <div class="row">
      <div class="col-sm-10 col-sm-offset-1 col-md-10 col-md-offset-1 main">
        <%= yield %>
      </div>
    </div>
  </div>

  <nav class="navbar" id="footer">
    <div class="container-fluid col-sm-10 col-sm-offset-1 col-md-10 col-md-offset-1 main">
      <div class="navbar-footer">
        <%= link_to emails_path, class: 'navbar-brand', style: "width: 200px;" do %>
          <img width="25" height="21" title="" alt="" src="data:image/gif;base64,"/><span class="visible-lg visible-md">EmailEngine</span>
        <% end %>
      </div>
      <div id="navbar">
        <ul class="nav navbar-nav navbar-left visible-lg visible-md" style="padding-top: 10px;">
          <li><span class="desc">Send and Track Emails at scale with Rails and Redis</span><br/><span class="copy">Copyright (c) 2016 Ty Rauber, MIT License</span></li>
        </ul>
        <ul class="nav navbar-nav navbar-right">
          <%= content_tag(:li, link_to("GitHub", "https://github.com/tyrauber/email_engine", target: "_blank"), style:"display:inline-block;") %>
          <%= content_tag(:li, link_to("Issues", "https://github.com/tyrauber/email_engine/issues", target: "_blank"), style:"display:inline-block;") %>
          <%= content_tag(:li, link_to("Wiki", "https://github.com/tyrauber/email_engine/wiki", target: "_blank"), style:"display:inline-block;") %>
        </ul>
      </div>
    </div>
  </nav>
</body>
</html>
